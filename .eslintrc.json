{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "no-underscore-dangle": "warn",
        "no-plusplus": "warn",
        "no-restricted-syntax": "warn",
        "no-setter-return": "warn",
        "no-return-assign": "warn",
        "no-unused-vars": "warn"
    }
}
